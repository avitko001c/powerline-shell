-def powerline_decode_error(e):
-	if not isinstance(e, UnicodeDecodeError):
-		raise NotImplementedError
-	return (''.join((
-		'<{0:02X}>'.format(c)
-		for c in tointiter(e.object[e.start:e.end])
-	)), e.end)
-
-
-codecs.register_error('powerline_decode_error', powerline_decode_error)
