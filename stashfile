-	def unichr(ch):
-		if ch <= sys.maxunicode:
-			return _unichr(ch)
-		else:
-			ch -= 0x10000
-			return _unichr((ch >> 10) + 0xD800) + _unichr((ch & ((1 << 10) - 1)) + 0xDC00)
